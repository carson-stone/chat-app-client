{
  "name": "chat-app-client",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carson-stone/chat-app-client.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carson-stone/chat-app-client/issues"
  },
  "homepage": "https://github.com/carson-stone/chat-app-client#readme",
  "eslintConfig": {
    "root": true,
    "plugins": [
      "@typescript-eslint/eslint-plugin",
      "eslint-plugin-vue"
    ],
    "overrides": [
      {
        "files": [
          "*.vue"
        ],
        "parser": "vue-eslint-parser",
        "parserOptions": {
          "parser": "@typescript-eslint/parser",
          "project": "./tsconfig.json",
          "ecmaVersion": 2020,
          "sourceType": "module",
          "extraFileExtensions": [
            ".vue"
          ]
        },
        "plugins": [
          "@typescript-eslint/eslint-plugin",
          "eslint-plugin-vue"
        ]
      }
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5050",
    "lint": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@vue/runtime-dom": "^3.2.31",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/node": "^16.11.25",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vitejs/plugin-vue": "^2.2.2",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.5.0",
    "tslib": "^2.3.1",
    "typescript": "~4.5.5",
    "vite": "^2.8.4",
    "vite-tsconfig-paths": "^3.4.1",
    "vue-eslint-parser": "^8.3.0"
  }
}
